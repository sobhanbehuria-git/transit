[
  {
    "name": "web",
    "image": "581641538996.dkr.ecr.us-east-1.amazonaws.com/web:latest",
    "memoryReservation": 512,
    "portMappings": [
      {
        "containerPort": 3000,
        "hostPort": 0,
        "protocol": "tcp"
      }
      
    ],
   "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/ecs/web-dev",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "ecs"
        }
    }
  }

 ]
